{
  "name": "jfaf",
  "version": "0.0.5",
  "description": "Builds a json folder structure with optional meta-data",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/dejongnj/jfaf"
  },
  "scripts": {
